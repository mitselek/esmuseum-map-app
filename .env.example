# Estonian War Museum Authentication Configuration
# Copy this file to .env and fill in your actual values

# Entu API Configuration
NUXT_ENTU_API_URL=https://your-entu-instance.com/api
NUXT_ENTU_CLIENT_ID=your_entu_oauth2_client_id

# Application Configuration
NUXT_PUBLIC_APP_URL=http://localhost:3000

# Note: NUXT_ENTU_CLIENT_SECRET not needed for direct frontend OAuth2 approach
# Only required if implementing server-side authorization code flow

# Development Configuration
NODE_ENV=development

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json

# Testing Configuration
TEST_ENTU_PERSON_ID=test_person_id_for_integration_tests
TEST_ENTU_GROUP_ID=test_group_id_for_integration_tests