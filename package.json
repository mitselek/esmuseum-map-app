{
  "name": "esmuseum-map-app",
  "private": true,
  "description": "ESM Map App",
  "author": "Argo Roots <argo@roots.ee>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "lint": "eslint . --fix --config .config/eslint.config.js",
    "dev": "nuxt dev --host 0.0.0.0",
    "dev:mobile": "nuxt dev --host 0.0.0.0 --port 3000",
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "start:mobile": "HOST=0.0.0.0 PORT=3000 node .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "analyze-translations": "node scripts/analyze-translations.cjs",
    "analyze-translations:report": "node scripts/analyze-translations.cjs --report",
    "analyze-translations:cleanup": "node scripts/analyze-translations.cjs --cleanup",
    "cleanup": "nuxt cleanup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:api": "vitest run tests/api",
    "test:composables": "vitest run tests/composables",
    "test:auth": "vitest run tests/unit/auth-*.test.ts tests/api/auth-*.test.ts tests/composables/*Auth*.test.ts",
    "test:coverage": "vitest run --coverage",
    "specify": "node scripts/spec-driven-workflow.js specify",
    "plan": "node scripts/spec-driven-workflow.js plan",
    "tasks": "node scripts/spec-driven-workflow.js tasks",
    "workflow": "node scripts/spec-driven-workflow.js",
    "validate:constitutional": "node scripts/constitutional-validator.js",
    "validate:typescript": "node scripts/constitutional-validator.js --typescript",
    "validate:coverage": "node scripts/constitutional-validator.js --coverage",
    "validate:eslint": "node scripts/constitutional-validator.js --eslint",
    "validate:all": "node scripts/constitutional-validator.js --all",
    "gate:specify": "node scripts/quality-gates.js specify",
    "gate:plan": "node scripts/quality-gates.js plan",
    "gate:tasks": "node scripts/quality-gates.js tasks",
    "setup:hooks": "bash scripts/setup-git-hooks.sh",
    "setup:constitutional": "npm run setup:hooks && npm run validate:all",
    "branch:feature": "node scripts/branch-manager.js create-feature",
    "branch:validate": "node scripts/branch-manager.js validate-current",
    "branch:switch": "node scripts/branch-manager.js switch",
    "branch:list": "node scripts/branch-manager.js list",
    "branch:setup": "node scripts/branch-manager.js setup",
    "github:setup": "node scripts/github-setup.js setup",
    "github:info": "node scripts/github-setup.js info",
    "github:templates": "node scripts/github-setup.js templates",
    "monitor": "node scripts/constitutional-monitoring.js monitor",
    "monitor:dashboard": "node scripts/constitutional-monitoring.js dashboard",
    "monitor:compliance": "node scripts/constitutional-monitoring.js compliance",
    "monitor:quality": "node scripts/constitutional-monitoring.js quality",
    "monitor:performance": "node scripts/constitutional-monitoring.js performance"
  },
  "engines": {
    "node": "22.x"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/i18n": "9.5.6",
    "@nuxtjs/tailwindcss": "6.14.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vueuse/nuxt": "13.5.0",
    "eslint": "9.30.1",
    "eslint-plugin-tailwindcss": "3.18.0",
    "msw": "^2.11.1",
    "naive-ui": "2.42.0",
    "nuxt": "3.17.6",
    "nuxt-icons": "3.2.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "dotenv": "^17.0.1",
    "js-yaml": "^4.1.0",
    "leaflet": "^1.9.4"
  }
}
