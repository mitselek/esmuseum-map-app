  "license": "MIT",
  "type": "module",
  "scripts": {
    "lint": "eslint . --fix --config .config/eslint.config.js",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "cleanup": "nuxt cleanup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:api": "vitest run tests/api", 
    "test:composables": "vitest run tests/composables",
    "test:auth": "vitest run tests/unit/auth-*.test.ts tests/api/auth-*.test.ts tests/composables/*Auth*.test.ts",
    "test:coverage": "vitest run --coverage"
  },
  "engines": {
    "node": "22.x"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.5.0",
    "@nuxt/scripts": "0.11.8",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/i18n": "9.5.6",
    "@nuxtjs/tailwindcss": "6.14.0",
    "@types/supertest": "^6.0.3",
    "@vueuse/nuxt": "13.5.0",
    "eslint": "9.30.1",
    "eslint-plugin-tailwindcss": "3.18.0",
    "msw": "^2.11.1",
    "naive-ui": "2.42.0",
    "nuxt": "3.17.6",
    "nuxt-icons": "3.2.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "dotenv": "^17.0.1",
    "js-yaml": "^4.1.0"
